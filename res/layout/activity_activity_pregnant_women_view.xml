<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:background="@color/background_theme_color"
    android:orientation="vertical"
    >

    <androidx.cardview.widget.CardView
        android:id="@+id/cvTitleBar"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:cardElevation="5dp"
        app:layout_constraintTop_toTopOf="parent">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="50dp"
            android:background="@color/toolbar_color"
            android:elevation="2dp"
            app:layout_constraintTop_toTopOf="parent">

            <ImageView
                android:id="@+id/ivTitleBack"
                android:layout_width="40dp"
                android:layout_height="40dp"
                android:layout_margin="10dp"
                android:elevation="2dp"
                android:src="@drawable/ic_baseline_keyboard_backspace_24"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <TextView
                android:id="@+id/tvTitleText"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:layout_margin="5dp"
                android:text=""
                android:textColor="@color/white"
                android:textSize="18sp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

        </androidx.constraintlayout.widget.ConstraintLayout>
    </androidx.cardview.widget.CardView>
<ScrollView
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#fff"
    android:padding="5dp"
    android:layout_below="@+id/cvTitleBar"
    android:scrollbars="none">
    
    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
       	android:orientation="vertical" >
        
        <LinearLayout
            android:layout_width="fill_parent"
        android:layout_height="fill_parent"
       	android:orientation="vertical"
       	android:background="@drawable/border"
       	 >
 <RelativeLayout
        android:id="@+id/pregWomantitle" 
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@color/background_theme_color"
        android:layout_alignParentTop="true">
    
    <TextView
        android:id="@+id/txtPregWomanProfile"
        android:layout_width="wrap_content"
        android:layout_height="fill_parent"
        android:layout_alignParentLeft="true"
        android:text="Pregnant Woman Profile"
        android:textColor="#fff"
        android:textSize="16sp" 
        android:paddingLeft="5dp"
        android:paddingTop="4dp"
        android:paddingBottom="4dp"/>
</RelativeLayout>
 <LinearLayout
     	android:id="@+id/lnrPregWomanProfile"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="5dp"
        android:layout_marginLeft="8dp"
        android:layout_marginBottom="15dp"
        android:layout_below="@+id/pregWomantitle"
        android:orientation="vertical"
        >
    
     <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal" >

            <TextView
                android:id="@+id/textNameView"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Name: "
                android:textColor="#000" />

            <TextView
                android:id="@+id/textName"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="2dp"
                android:textColor="#000" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal" >

            <TextView
                android:id="@+id/textAddressView"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Address: "
                android:textColor="#000" />

            <TextView
                android:id="@+id/textAddress"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="2dp"
                android:textColor="#000" />
        </LinearLayout>
        
 </LinearLayout>
 </LinearLayout>
 <LinearLayout
     android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:layout_marginTop="4dp"
       	android:orientation="vertical"
       	android:background="@drawable/border"
       	>
 <RelativeLayout
        android:id="@+id/familyTitle" 
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@color/background_theme_color"
       	android:layout_below="@+id/lnrPregWomanProfile">
    
    <TextView
        android:id="@+id/txtFamilyProfile"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:text="Family Details"
        android:textColor="#fff"
        android:textSize="16sp" 
        android:paddingLeft="5dp"
        android:paddingTop="4dp"
        android:paddingBottom="4dp"/>
</RelativeLayout>
 <LinearLayout
     android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="5dp"
        android:layout_marginLeft="8dp"
        android:layout_marginBottom="15dp"
        android:layout_below="@+id/familyTitle"
        android:orientation="vertical"
        >
    
     <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal" >

            <TextView
                android:id="@+id/textFatherNameView"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Father/ Husband Name: "
                android:textColor="#000" />

            <TextView
                android:id="@+id/textFatherName"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="2dp"
                android:textColor="#000" />
        </LinearLayout>

     <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal" >

            <TextView
                android:id="@+id/textAdhaarCardView"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Adhaar Card: "
                android:textColor="#000" />

            <TextView
                android:id="@+id/textAdhaarCard"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="2dp"
                android:textColor="#000" />
        </LinearLayout>
     
     
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal" >

            <TextView
                android:id="@+id/textReligionView"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Religion: "
                android:textColor="#000" />

            <TextView
                android:id="@+id/textReligion"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="2dp"
                android:textColor="#000" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:visibility="gone"
            android:orientation="horizontal" >

            <TextView
                android:id="@+id/textCastCatView"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Cast Category: "
                android:textColor="#000" />

            <TextView
                android:id="@+id/textCastCat"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="2dp"
                android:textColor="#000" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal" >

            <TextView
                android:id="@+id/textToiletView"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Have Toilet: "
                android:textColor="#000" />

            <TextView
                android:id="@+id/textToilet"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="2dp"
                android:textColor="#000" />
        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal" >

            <TextView
                android:id="@+id/textWaterView"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Water: "
                android:textColor="#000" />

            <TextView
                android:id="@+id/textWater"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="2dp"
                android:textColor="#000" />
        </LinearLayout>
 </LinearLayout>
 </LinearLayout>
 <LinearLayout
     android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:layout_marginTop="4dp"
       	android:orientation="vertical"
       	android:background="@drawable/border"
       	>
  <RelativeLayout
      android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@color/background_theme_color"
       	android:layout_below="@+id/lnrPregWomanProfile">
    
    <TextView
        android:id="@+id/txtGrowthHistory"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:text="Growth History"
       	android:textColor="#fff"
        android:textSize="16sp" 
        android:paddingLeft="5dp"
        android:paddingTop="4dp"
        android:paddingBottom="4dp" />
</RelativeLayout>
<LinearLayout
    android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="2dp"
        android:layout_marginBottom="15dp"
        android:layout_marginLeft="2dp"
        android:layout_marginRight="2dp"
        android:layout_below="@+id/txtGrowthHistory"
        android:orientation="vertical">
    
    
    <WebView 
        android:id="@+id/wvNutritionHistory"
        android:layout_width="wrap_content"
        android:layout_height="match_parent"/>
        </LinearLayout>
       
</LinearLayout>
</LinearLayout>
</ScrollView>
</RelativeLayout>
